<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Beta>False</Beta>
  <Category>Network:Management</Category>
  <Name>Gitlab</Name>
  <Description>
    [h3]Gitlab[/h3] Git server and web UI.[br][br]
    [b][span style=\'color: #E80000;\']Directions:[/span][/b][br]
    [b]/var/log/gitlab[/b] : Logs.[br]
    [b]/var/opt/gitlab[/b] : Application data.[br]
	[b]/etc/gitlab[/b] : Configuration.[br]

	Default access username [b]root[/b] and password [b]5iveL!fe[/b]
  </Description>
  <Registry>https://registry.hub.docker.com/u/sytse/gitlab-ce/</Registry>
  <Repository>sytse/gitlab-ce:7.10.1</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8095</HostPort>
        <ContainerPort>80</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
	  <Port>
        <HostPort>2222</HostPort>
        <ContainerPort>22</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/gitlab/logs</HostDir>
      <ContainerDir>/var/log/gitlab</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
	<Volume>
      <HostDir>/mnt/user/appdata/gitlab/data</HostDir>
      <ContainerDir>/var/opt/gitlab</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
	<Volume>
      <HostDir>/mnt/user/appdata/gitlab/conf</HostDir>
      <ContainerDir>/etc/gitlab</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Version></Version>
  <WebUI>http://[IP]:[PORT:8095]/</WebUI>
  <Banner>http://i.imgur.com/eddJlVg.png</Banner>
  <Icon>http://i.imgur.com/KKoQMo6.png</Icon>
</Container>
